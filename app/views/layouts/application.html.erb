<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= auto_discovery_link_tag(:atom, posts_path(:atom)) %>
</head>
<body>
  <div class="wrap">
  	<%= render :partial => 'layouts/header' %>
  	<div class="main">
  		<%= yield %>
  	</div>
  	<%= render :partial => 'layouts/footer' %>
  </div>
</body>
</html>
